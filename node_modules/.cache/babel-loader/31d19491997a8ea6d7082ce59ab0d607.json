urceRoot":"","sources":["C:\\Users\\小宋\\Desktop\\blog\\src\\views\\Pag\\PagNav.vue"],"sourcesContent":["<template>\r\n    <el-menu\r\n   class=\"nav\"\r\n   mode=\"vertical\"\r\n   :default-active=\"id\"\r\n   router\r\n   >\r\n    <el-menu-item\r\n    v-if=\"key.key\"\r\n    :index=\"key.url\">\r\n        {{key.key}}\r\n    </el-menu-item>\r\n   <div v-for=\"item in list\"\r\n    :key=\"item.id\">\r\n    <el-sub-menu\r\n    :index=\"item.id\"\r\n    > \r\n    <template #title>\r\n        <span>{{item.id}}</span>\r\n    </template>\r\n    <el-menu-item\r\n    v-for=\"items in item.nav\"\r\n    :key=\"items.name\"\r\n    :index=\"key.url+'/'+items.id\" \r\n    >\r\n    {{items.name}}\r\n    </el-menu-item>\r\n    </el-sub-menu>\r\n    </div>\r\n   </el-menu>\r\n</template>\r\n\r\n<script>\r\nimport { ref } from '@vue/reactivity'\r\nimport { get } from '../../assets/requr'\r\nimport { useRoute } from 'vue-router'\r\n\r\nexport default {\r\n    name: 'PagNav',\r\n    setup () {\r\n    const route = useRoute()\r\n    // const router = useRouter()\r\n    const list = ref([])\r\n    const key = ref([])\r\n    const nearbyList = async () => {\r\n    const result = await get(`${route.params.navId}`)\r\n    if (result?.data ){\r\n        list.value   = result.data\r\n        key.value = result.meta\r\n      }\r\n    }\r\n    nearbyList()\r\n    const id = decodeURI(route.path)\r\n    return { list, key, id }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.nav{\r\n    width: 2.5rem;\r\n    margin: .1rem .2rem;\r\n    overflow-y: auto;\r\n    float: left;\r\n}\r\n.el-menu-item.is-active {\r\n    background-color: #e1f1ff;\r\n    border-left: 2px solid #39a3ff;\r\n}\r\n</style>"]}]}